<p id="notice"><%= notice %></p>

<h1>Listing Uf Prices</h1>

<table>
  <thead>
    <tr>
      <th>Uf price</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @uf_prices.each do |uf_price| %>
      <tr>
        <td><%= uf_price.uf_price %></td>
        <td><%= link_to 'Show', uf_price %></td>
        <td><%= link_to 'Edit', edit_uf_price_path(uf_price) %></td>
        <td><%= link_to 'Destroy', uf_price, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Uf price', new_uf_price_path %>
